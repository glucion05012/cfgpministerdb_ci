CREATE TABLE ordination (
    ord_id INTEGER(20) AUTO_INCREMENT NOT NULL,
    ord_min_id INTEGER(20),
    ord_father_name VARCHAR(200),
    ord_mother_name VARCHAR(200),
    ord_solemnizing_official VARCHAR(200),
    ord_solemnizing_place VARCHAR(200),
    ord_if_separated VARCHAR(200),
    ord_if_engaged_to VARCHAR(200),
    ord_if_engaged_christian VARCHAR(200),
    ord_if_engaged_member VARCHAR(200),
    ord_if_engaged_symphaty VARCHAR(200),
    ord_if_engaged_not_symphaty VARCHAR(200),
    ord_if_engaged_minister VARCHAR(200),
    ord_if_engaged_graduate_fbc VARCHAR(200),
    ord_if_engaged_when_married VARCHAR(200),
    ord_course_bible_college VARCHAR(200),
    ord_year_conversion_when VARCHAR(200),
    ord_year_conversion_where VARCHAR(200),
    ord_year_baptized_when VARCHAR(200),
    ord_year_baptized_where VARCHAR(200),
    ord_testimony VARCHAR(500),
    ord_ministerial_activities VARCHAR(500),
    ord_churches_pioneered VARCHAR(500),
    ord_churches_evangelized VARCHAR(500),
    ord_resigned_pastorate VARCHAR(500),
    ord_fulltime_ministry VARCHAR(200),
    ord_secular_employment VARCHAR(500),
    ord_foursquare_credentials VARCHAR(500),
    ord_other_licensed VARCHAR(500),
    ord_pending_application VARCHAR(500),
    ord_harmony_authority VARCHAR(200),
    ord_pledged_foursquare VARCHAR(200),
    ord_teaching_principles VARCHAR(200),
    ord_teach_tithing VARCHAR(200),
    ord_consistent_tither VARCHAR(200),
    ord_ministry_support VARCHAR(500),
    ord_declaration_faith VARCHAR(200),
    ord_way_salvation VARCHAR(500),
    ord_holy_trinity VARCHAR(500),
    ord_eternal_security VARCHAR(500),
    ord_date_submitted VARCHAR(200),
    ord_district_status VARCHAR(200),
    ord_district_interview VARCHAR(200),
    ord_national_status VARCHAR(200),
    ord_national_exam VARCHAR(200),
    ord_national_interview VARCHAR(200),
    ord_remarks VARCHAR(200),
    PRIMARY KEY (ord_id)
);
